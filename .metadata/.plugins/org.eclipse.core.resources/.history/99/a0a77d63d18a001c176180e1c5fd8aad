package entities;

public class Container {
	
	private String empresa;
	private String numero;
	private String missao;
	private double peso;
	
	public Container() {
		
	}

	public Container(String empresa, String numero, String missao, double peso) {
		super();
		this.empresa = empresa;
		this.numero = numero;
		this.missao = missao;
		this.peso = peso;
	}
	
	
}
